{
  "name": "@rustpress/visual-queue-manager",
  "version": "1.0.0",
  "private": true,
  "description": "Visual Queue Manager frontend for RustPress",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
